<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#ececec"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".mainpages.mypage.mypage">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="195dp">

        <TextView
            android:id="@+id/tv_nick"
            android:layout_width="163dp"
            android:layout_height="35dp"
            android:clickable="true"
            android:gravity="left"
            android:text="nick_name"
            android:textColor="#000000"
            android:textSize="24sp"
            android:visibility="visible"
            app:layout_constraintBottom_toTopOf="@+id/mypage_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.81"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.289"
            tools:visibility="visible" />

        <ImageView
            android:id="@+id/user_image"
            android:layout_width="84dp"
            android:layout_height="86dp"
            android:scaleType="fitXY"
            app:layout_constraintBottom_toTopOf="@+id/mypage_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.275"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/user_no_image"
            tools:ignore="VectorDrawableCompat" />

        <TextView
            android:id="@+id/follow"
            android:layout_width="66dp"
            android:layout_height="30dp"
            android:clickable="true"
            android:gravity="center"
            android:text="팔로우"
            android:textColor="#000000"
            android:visibility="visible"
            app:layout_constraintBottom_toTopOf="@+id/mypage_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.544"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.507"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/follower"
            android:layout_width="66dp"
            android:layout_height="30dp"
            android:clickable="true"
            android:gravity="center"
            android:text="팔로워"
            android:textColor="#000000"
            android:visibility="visible"
            app:layout_constraintBottom_toTopOf="@+id/mypage_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.736"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.515"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/follow2"
            android:layout_width="59dp"
            android:layout_height="26dp"
            android:clickable="true"
            android:gravity="center"
            android:text="0"
            android:textColor="#000000"
            android:visibility="visible"
            app:layout_constraintBottom_toTopOf="@+id/mypage_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.534"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.73"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/follower2"
            android:layout_width="61dp"
            android:layout_height="27dp"
            android:clickable="true"
            android:gravity="center"
            android:text="0"
            android:textColor="#000000"
            android:visibility="visible"
            app:layout_constraintBottom_toTopOf="@+id/mypage_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.74"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.736"
            tools:visibility="visible" />

        <ImageView
            android:id="@+id/setting"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:clickable="true"
            app:layout_constraintBottom_toTopOf="@+id/mypage_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.98"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.04000002"
            app:srcCompat="@drawable/setting_icon" />

        <ImageView
            android:id="@+id/mypage_button"
            android:layout_width="498dp"
            android:layout_height="44dp"
            android:scaleType="fitXY"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.505"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/mypage_button"
            tools:ignore="VectorDrawableCompat" />

        <ImageView
            android:id="@+id/capsule"
            android:layout_width="35dp"
            android:layout_height="37dp"
            android:scaleType="fitXY"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.26"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.98"
            app:srcCompat="@drawable/mypage_capsule2"
            tools:ignore="VectorDrawableCompat" />

        <ImageView
            android:id="@+id/capsule_map"
            android:layout_width="35dp"
            android:layout_height="37dp"
            android:scaleType="fitXY"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.728"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.98"
            app:srcCompat="@drawable/mypage_capsule_map"
            tools:ignore="VectorDrawableCompat" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <fragment
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/bottom_nav"
            android:layout_alignParentTop="true"
            android:layout_marginTop="0dp"
            android:layout_marginBottom="-56dp"
            tools:context=".MapsActivity"
            tools:ignore="NotSibling" />

        <com.google.android.material.bottomnavigation.BottomNavigationView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/bottom_nav"
            app:itemTextColor="@drawable/nav_selector"
            app:itemIconTint="@drawable/nav_selector"
            android:background="@drawable/nav_border"
            app:menu="@menu/menu_nav"
            android:layout_alignParentBottom="true" />

    </RelativeLayout>

</androidx.appcompat.widget.LinearLayoutCompat>